.pyg_index swiper{
  width: 750rpx;
  height: 340rpx;
}
.pyg_index swiper image{
  width: 100%;
}
text{
  color: var(--themeColor);
}
.cate_index{
  display: flex;
}
.cate_index navigator{
  flex: 1;
}
.cate_index navigator image{
    width: 100%;
}
.floor_title{
  padding: 10rpx 0;
}
.floor_title image{
  width: 100%;
}
.floor-list{
  overflow: hidden;
}
.floor-list navigator{
  float: left;
  width: 33.33%;
}
.floor-list navigator image{
  width: 100%;
  height: 100%;
}
.floor-list navigator:nth-last-child(-n+4){
  /* 232/386=33vw/height */
  height: calc(33.33vw * 386 / 232 / 2 );
  /* height: 27.72711207vw; */
  border-left: 10rpx solid white;
}
.floor-list navigator:nth-last-child(3),.floor-list navigator:nth-last-child(4){
  border-bottom: 10rpx solid white;
}