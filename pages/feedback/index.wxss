page{
    /* padding: 20rpx; */
    background-color: #eeeeee;
}
.fb_main{
    padding: 20rpx;
    color: #666;
}
.fb_main .tips{
    display: flex;
    flex-wrap: wrap;
}
.fb_main .tips .tips_item{
    width: 30%;
    padding: 10rpx;
    text-align: center;
    background-color: #fff;
    margin: 20rpx 10rpx;
}
.fb_content{
    background-color: #fff;
    margin-top: 20rpx;
}
.fb_content textarea{
    padding: 10rpx;
}
.fb_tool{
    padding-bottom: 30rpx;
}
.fb_tool button{
    width: 90rpx;
    height: 90rpx;
    font-size: 60rpx;
    padding: 0;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-left: 20rpx;
}
.form_btn_wrap{
    margin-top: 20rpx;
    display: flex;
    justify-content: flex-end;
    align-items: center;
    text-align: center;
}
.form_btn_wrap icon{
    vertical-align: middle;
}
.form_btn_wrap button{
    margin: 0;
    /* height: 70rpx; */
    width: 30%;
    text-align: center;
}
.fb_tool button{
    margin: 0;
}
.fb_tool{
    display: flex;
}
.up_img_item{
    margin-left: 20rpx;
    height: 90rpx;
    width: 90rpx;
}

