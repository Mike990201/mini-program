<view class="tabs">
    <view class="tabs_title">
        <view class="tabs_item {{item.isActive?'active':''}}" wx:for="{{tabs}}" wx:key="id" bindtap="bindTabsItemchange" data-index="{{index}}">
            {{item.value}}
        </view>
    </view>
    <view class="content">
     <slot></slot>
    </view>
</view>